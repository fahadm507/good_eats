<!DOCTYPE html>
<html>
<head>
  <title>GoodEats</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
  <% if flash.any? %>
    <div class="notice">
      <p><%= flash[:notice] %></p>
    </div>
  <% end %>
  <%= yield %>
</div>

</body>
</html>
